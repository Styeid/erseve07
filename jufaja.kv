<Button>
    id: buttonss
    opacity: 0.97
    font_name: "nasaliza"
    font_size: 37
####   Eigenschappen   ####
    #text: "V4"
    #text_size: self.size
    halign: "center"
    valign: "bottom"
    #color: (79/255, 79/255, 79/255, 1)
    background_normal: ""
    background_color: (77/255, 0/255, 170/255, .1)

    #bold: True
    italic: True
    #outline_color: (0, 0, 0)
    #outline_width: 0

<ControlXButton@Button>
    font_name: "seguisym"
    background_normal: ""
    background_color: (240/255, 240/255, 240/255, 0)
    canvas.before:
        Color:
            rgba: (0/255, 10/255, 90/255, 0)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [70]

<Piont@Label>
    font_name: "seguisym"
