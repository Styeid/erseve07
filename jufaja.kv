MyLayoutYo:
    RelativeLayout:
        id: STart_BUT
        canvas.before:
            Color:
                rgba: 0, 0, 0, 0.4
            Rectangle:
                pos: self.pos
                size: self.size

        Label:
            id: nu_of_twists
            pos_hint: {"center_x": 0.27, "center_y": 0.94}
            size_hint: 0.025, 1
            font_name: "nasaliza"
            text: str(root.NB_of_TWISTs)
            color: (111/255, 213/255, 245/255, 1)
            font_size: root.width * 0.037
            bold: False
            italic: True
            outline_color: (233/255, 46/255, 251/255)
            outline_width: 0
        Button:
            id: minx
            disabled: False
            font_name: "seguisym"
            text: u"\uE016"
            color: (0/255, 255/255, 255/255, 1)
            font_size: root.width * 0.015
            background_normal: ""
            background_color: (1/255, 220/255, 255/255, 0.5)
            pos_hint: {"center_x": 0.087, "center_y": 0.94}
            size_hint: 0.1, 0.09
            on_release: root.top_colorstonex(self)
            on_press: root.progress_minus(), root.min_m_twists()
        Button:
            id: plux
            disabled: False
            font_name: "seguisym"
            text: u"\uE017"
            color: (0/255, 255/255, 255/255, 1)
            font_size: root.width * 0.015
            background_normal: ""
            background_color: (1/255, 220/255, 255/255, 0.5)
            pos_hint: {"center_x": 0.19, "center_y": 0.94}
            size_hint: 0.1, 0.09
            on_release: root.top_colorstonex(self)
            on_press: root.progress_plus(), root.m_max_twists()
        ProgressBar:
            id: progress_twists
            value: root.NB_of_TWISTs
            min: 0
            max: root.MAX_twists
            pos_hint: {"center_x": 0.4, "center_y": 0.94}
            size_hint_x: 0.2
            background_color: (255/255, 32/255, 121/255, 1)
        Label:
            id: ratingY
            font_name: "neon2"
            text: ""
            color: (212/255, 175/255, 55/255, 1)
            font_size: root.width * 0.12
            outline_color: (111/255, 213/255, 245/255)
            outline_width: 2
            pos_hint: {"center_x": 0.13, "center_y": 0.74}
        Label:
            id: ratingX
            font_name: "nasaliza"
            text: ""
            color: (212/255, 175/255, 55/255, 1)
            font_size: root.width * 0.02
            outline_color: (111/255, 213/255, 245/255)
            outline_width: 0
            pos_hint: {"center_x": 0.13, "center_y": 0.63}
        Label:
            id: home_one
            text: "J U F A J A  Grid"
            font_name: "nasaliza"
            font_size: root.width * 0.09
            outline_color: (233/255, 46/255, 251/255)
            outline_width: 2
            color: (255/255, 255/255, 255/255, 0.79)
            pos_hint: {"center_x": 0.5, "center_y": 0.45}
        Label:
            text: "www.jufaja1@outlook.com"
            font_name: "seguisym"
            font_size: root.width * 0.017
            outline_color: (255/255, 70/255, 90/255)
            outline_width: 1
            color: (200/255, 200/255, 200/255, 0.3)
            pos_hint: {"right": 1.37, "center_y": 0.29}
        Button:
            id: home_two
            text: "Start"
            font_name: "nasaliza"
            font_size: root.width * 0.02
            background_normal: ""
            background_color: (255/255, 70/255, 90/255, 0.5)
            size_hint: 0.1, 0.07
            disabled: True
            pos_hint: {"center_x": 0.06, "center_y": 0.083}
            on_press: root.start_button_start()
        ControlXButton:
            id: home_twob
            text: "\n\nExit Game"
            color: 240/255, 240/255, 240/255, 0.5
            font_name: "nasaliza"
            font_size: root.width * 0.02
            size_hint: 0.15, 0.15
            disabled: False
            pos_hint: {"center_x": 0.7, "center_y": 1}
            canvas.before:
                Color:
                    rgba: (230/255, 10/255, 40/255, 0.47)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [40]
            on_press: root.close_win()
        Button:
            id: home_three
            text: ""
            font_name: "nasaliza"
            font_size: root.width * 0.02
            background_normal: ""
            background_color: (255/255, 70/255, 90/255, 0.5)
            size_hint: 0.1, 0.07
            disabled: True
            pos_hint: {"center_x": 0.2, "center_y": 0.083}
            on_press: root.start_button_win()
