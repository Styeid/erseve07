#:import Factory kivy.factory.Factory

<PopupXbutton@Button>
    pos_hint: {"center_x": 0.94, "center_y": 0.164}
    size_hint: 0.09, 0.1
    font_name: "ttf/nasaliza"
    font_size: root.width * .25
    background_normal: ""
    background_color: (240/255, 240/255, 240/255, 0)
    canvas.before:
        Color:
            rgba: (255/255, 70/255, 90/255, 1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [1]

<PopupXLabel@Label>
